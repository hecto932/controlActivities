{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "controlActivities",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/destinations/SimpleAgro/services/avicola.xsodata/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "webapp/localService/avicola.xsodata/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "controlActivities.view.App",
			"type": "JS"
		},
		"handleValidation": true,
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"": {
				"dataSource": "mainService"
			},
			"jsonmodel": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/data.json"
			},
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "controlActivities.i18n.i18n"
				}
			},
			"dummy": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/dummy.json"
			},
			"chart": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/chart.json"
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewPath": "controlActivities.view",
				"viewType": "XML",
				"clearTarget": false,
				"targetAggregation": "pages",
				"transition": "slide",
				"targetControl": "app",
				"bypassed": {
					"target": "notFound"
				}
			},
			"routes": [
				{
					"pattern": "",
					"name": "home",
					"view": "Shed",
					"targetAggregation": "pages",
					"targetControl": "app"
				},
				{
					"pattern": "splitApp",
					"name": "splitapp",
					"view": "SplitApp",
					"targetControl": "app",
					"subroutes": [
						{
							"pattern": "master/{lotId}/{shedId}",
							"name": "master",
							"view": "Master",
							"targetAggregation": "masterPages",
							"targetControl": "splitapp"
						},
						{
							"pattern": "detail/{lotId}/{shedId}/{weekId}",
							"name": "detail",
							"view": "Detail",
							"targetAggregation": "detailPages",
							"targetControl": "splitapp",
							"subroutes": [
								{
									"pattern": "weight/{shedId}/{weekId}",
									"name": "weight",
									"view": "Weight",
									"targetAggregation": "pages",
									"targetControl": "app"
								}
							]
						},
						{
							"pattern": "graphic",
							"name": "graphic",
							"view": "Chart",
							"targetAggregation": "pages",
							"targetControl": "app"
						}
					]
				}
			],
			"targets": {
				"home": {
					"viewName": "Shed",
					"viewType": "XML",
					"viewLevel": 1
				},
				"notFound": {
					"viewName": "NotFound",
					"viewType": "XML",
					"transition": "slide"
				},
				"splitapp": {
					"viewName": "SplitApp",
					"viewType": "XML",
					"viewLevel": 2
				},
				"master": {
					"viewName": "Master",
					"viewType": "XML",
					"viewLevel": 3
				},
				"detail": {
					"viewName": "Detail",
					"viewType": "XML",
					"viewLevel": 4
				},
				"graphic": {
					"viewName": "Chart",
					"viewType": "XML",
					"viewLevel": 5
				},
				"weight": {
					"viewName": "Weight",
					"viewType": "XML",
					"viewLevel": 5
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		}
	}
}
